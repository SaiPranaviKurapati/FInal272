{
  "name": "project-tracker",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run start:ng\" \"npm run start:flask\"",
    "build":"NODE_OPTIONS=--max_old_space_size=8192 ng build --aot",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:ng": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve"
